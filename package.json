{
  "name": "diesal",
  "version": "0.3.2",
  "description": "Data structures and algorithms in ES2015 modules",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "babel -d lib/ src/",
    "update-docs": "esdoc",
    "test-single-run": "mocha --require babel-register --recursive",
    "test-watch": "npm run test-single-run -- -w",
    "coverage": "cross-env NODE_ENV=test nyc npm run test-single-run",
    "lint": "eslint src && eslint test",
    "test": "npm run lint && npm run coverage"
  },
  "keywords": [
    "data structures",
    "algorithms",
    "ecmascript",
    "modules"
  ],
  "author": "Jonathan Skeate",
  "license": "ISC",
  "repository": "http://github.com/skeate/diesal",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "chai": "^4.0.0",
    "codeclimate-test-reporter": "^0.4.0",
    "cross-env": "^6.0.0",
    "esdoc": "^0.5.2",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^6.10.3",
    "mocha": "^3.4.2",
    "nyc": "^10.3.2"
  },
  "eslintConfig": {
    "extends": [
      "airbnb"
    ],
    "plugins": [
      "mocha"
    ],
    "rules": {
      "mocha/max-top-level-suites": "error",
      "no-mixed-operators": "off",
      "no-underscore-dangle": "off",
      "no-plusplus": "off",
      "no-unused-vars": [
        "warn",
        {
          "vars": "local",
          "args": "none"
        }
      ],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ]
    }
  },
  "babel": {
    "presets": [
      "env"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "esdoc": {
    "source": "./src",
    "destination": "./out"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
